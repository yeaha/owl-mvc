{
    "name": "yeaha/owl-mvc",
    "description": "Owl Framework MVC Component",
    "keywords": ["mvc", "swoole", "restful", "psr-7", "middleware"],
    "homepage": "https://github.com/yeaha/owl-mvc",
    "license": "MIT",
    "authors": [
        {
            "name": "YangYi",
            "email": "yangyi.cn.gz@gmail.com",
            "homepage": "https://github.com/yeaha"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/http-message": "^1.0",
        "yeaha/owl-core": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Owl\\Context\\": "src/Context/",
            "Owl\\Http\\": "src/Http/",
            "Owl\\Mvc\\": "src/Mvc/",
            "Owl\\Swoole\\": "src/Swoole/",
            "Owl\\Traits\\": "src/Traits/",
            "Owl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
